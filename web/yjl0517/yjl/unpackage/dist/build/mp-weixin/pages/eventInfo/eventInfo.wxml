<view class="container"><view class="view_name"><text>发起物资调用</text></view><view class="dataBox"><view class="dataBox_title">{{'事件编号：'+code+''}}</view><view class="dataBox_address"><view class="address_label"><text class="label">事件地址：</text><text data-event-opts="{{[['tap',[['copyAddress',['$event']]]]]}}" class="copyBtn" bindtap="__e">复制地址</text></view><view class="dataBox_value">{{''+data.address+''}}</view></view><view data-event-opts="{{[['tap',[['openMap',['$event']]]]]}}" class="map" bindtap="__e"><map style="width:562rpx;height:248rpx;" latitude="{{latitude}}" longitude="{{longitude}}" markers="{{covers}}"></map></view><view class="remark"><text>{{"事件备注: "+data.remark}}</text></view></view><view class="btnGroup"><block wx:if="{{$root.m0}}"><view data-event-opts="{{[['tap',[['toAdd',['$0'],['code']]]]]}}" class="btn toAdd" bindtap="__e">发起物资调用申请</view></block><block wx:if="{{$root.m1}}"><view data-event-opts="{{[['tap',[['toList',['$0'],['code']]]]]}}" class="btn toList" bindtap="__e">查看物资调用申请</view></block></view></view>